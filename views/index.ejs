
<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>Table Example</title>
    <link rel="stylesheet" type="text/css" href="/style.css">
    <style>
      table {
  border-collapse: collapse;
  width: 100%;
  max-width: 800px;
  margin: 0 auto;
}

th,
td {
  border: 1px solid #ddd;
  padding: 8px;
  text-align: left;
}

th {
  background-color: #4CAF50;
  color: white;
}
    </style>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>SAP Code</th>
          <th>Description</th>
          <th>Quantity</th>
        </tr>
      </thead>
      <tbody>
        <% if (data && data.row) { %>
          <% data.row.forEach(item=> { %>
            <tr>
              <td><%= item.sap_code %></td>
              <td><%= item.description %></td>
              <td><%= item.quantity %></td>
            </tr>
          <% }); %>
        <% } %>
      </tbody>
    </table>    
    <a href="/download-pdf" target="_blank">Download PDF</a>
    <a href="/upload-pdf" target="_blank">Upload PDF to FTP</a>
  </body>
</html>
